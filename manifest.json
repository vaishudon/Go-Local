{
  "name": "Go Local",
  "description": "SWE Hacks '22",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://127.0.0.1:5000/*",
    "activeTab",
    "http://api.openweathermap.org/data/2.5/*",
    "geolocation",
    "https://api.waqi.info//feed/*"
  ],
  "icons": {
    "16": "logo-16.png",
    "128": "logo-128.png"
  }
}